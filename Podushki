

#include <iostream>

using namespace std;

int main()
{
    setlocale(LC_ALL, "rus");
    int L;
    int P;
    int podushki = 0;


    cout << "Введите длину ткани: ";
    cin >> L;
    cout << "Введите длину подушки: ";
    cin >> P;
    if (L > 0)
    {
        for (int i = 0; i < L;)
        {
            podushki += 1;
            i += P;
        }
        cout << podushki;
    }
    else
    {
        cout << "Ткани нет!!!";
    }
}
