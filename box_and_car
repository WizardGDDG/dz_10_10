
#include <iostream>

using namespace std;

int main()
{
    setlocale(LC_ALL, "rus");
    int n;
    int colichestwo;
    int car = 0;
    

    cout << "Введите сколько ящиков на складе: ";
    cin >> n;
    cout << "Введите сколько может помещать ящиков машина: ";
    cin >> colichestwo;
    if (n>0)
    {
        for (int i = 0; i < n;)
        {
            car += 1;
            i += colichestwo;
        }
        cout << car;
    }
    else 
    {
        cout << "Так, ящиков нет";
    }
}


